				$('.keyword-input-text').val('');
				$('.keyword-input-text').css('width', $('.hidden-text2').width() + 'px');
				inputFocusout();
			}

			$('.hashtag-info strong em').css('display', 'none');

			tagCountNumber();
			maximumTag();

		}

